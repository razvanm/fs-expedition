one() {
    for i in `seq 0 29`
    do
	echo 2.6.$i
	../summaryone.py ../linux-2.6.$i.obj | ./heatmap-hamm-fixed.py "$2" > heatmap-2.6.$i
    done
    
    R --no-save < heatmaps.R
    
    convert -delay 20 heatmap.hamm.2.6.?.png heatmap.hamm.2.6.??.png -delay 500 heatmap.hamm.2.6.29.png heatmap.hamm.gif
    [ -d $2 ] || mkdir $1
    mv *.png $1
    mv *.gif $1
}

